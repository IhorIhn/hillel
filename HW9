Task1:

// Створення об'єкта "Людина"
let person = {
  year: 1990,
  firstName: 'Олена',
  lastName: 'Теліга',
  getInfo: function() {
    for (let key in this) {
      if (typeof this[key] !== 'function') {
        console.log(`${key}: ${this[key]}`);
      }
    }
  },
  getFullName: function() {
    console.log(`Повне ім'я: ${this.firstName} ${this.lastName}`);
  }
};

// Виведення інформації та повного імені
person.getInfo();
person.getFullName();

