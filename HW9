Task2

function isARobot(object) {
    return object.hasOwnProperty('batteryEnergy');
  }
  
  // Приклади об'єктів
  let robot1 = {
    batteryEnergy: 100,
    name: 'Робот 1'
  };
  
  let human = {
    name: 'Олена'
  };
  
  let robot2 = {
    batteryEnergy: 50,
    model: 'SpaceX'
  };
  
  // Перевірка чи є об'єкти роботами
  console.log(isARobot(robot1)); // Повинно повернути true
  console.log(isARobot(human)); // Повинно повернути false
  console.log(isARobot(robot2)); // Повинно повернути true
  
